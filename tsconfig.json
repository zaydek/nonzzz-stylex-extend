{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "dist",
    "noImplicitThis": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["ESNext", "DOM"],
    "jsx": "react-jsx",
    "moduleResolution": "Bundler",
    "types": ["vite/client", "react", "react-dom", "@stylex-extend/react"]
  }
}
